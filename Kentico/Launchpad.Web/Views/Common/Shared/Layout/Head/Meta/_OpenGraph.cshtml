@*
    Built with Common Launchpad 2.0.2
*@

@model BaseViewModel

@if (!string.IsNullOrWhiteSpace(Model.Metadata.OgTitle))
{
    <meta property="og:title" content="@Model.Metadata.OgTitle" />
}
@if (!string.IsNullOrWhiteSpace(Model.Metadata.OgDescription))
{
    <meta property="og:description" content="@Model.Metadata.OgDescription" />
}
@if (!string.IsNullOrWhiteSpace(Model.Metadata.OgUrl))
{
    <meta property="og:url" content="@Model.Metadata.OgUrl" />
}
@if (!string.IsNullOrWhiteSpace(Model.Metadata.OgImage))
{
    <meta property="og:image" content="@Model.Metadata.OgImage" />
}
@if (!string.IsNullOrWhiteSpace(Model.Metadata.TwitterCard))
{
    <meta name="twitter:card" content="@Model.Metadata.TwitterCard">
}
@if (!string.IsNullOrWhiteSpace(Model.Metadata.TwitterSite))
{
    <meta name="twitter:site" content="@Model.Metadata.TwitterSite">
}
@if (!string.IsNullOrWhiteSpace(Model.Metadata.TwitterCreator))
{
    <meta name="twitter:creator" content="@Model.Metadata.TwitterCreator">
}