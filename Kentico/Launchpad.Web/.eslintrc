{
	"parser": "babel-eslint",
	"extends": [ "airbnb" ],
	"rules": {
		"indent": 0,
		"no-tabs": 0,
		"no-useless-constructor": 0,
		"no-empty-function": 0,
		"no-trailing-spaces": 0,
		"eol-last": 0,
		"no-multi-assign": 0,
		"global-require": 0,
		"class-methods-use-this": 0,
		"arrow-parens": 0,
		"object-curly-newline": 0,
		"no-multi-spaces": 0,
		"comma-dangle": 0,
		"no-undef": 0,
		"no-unused-vars": 0,
		"brace-style": 0,
		"no-restricted-syntax": 0,
		"linebreak-style": 0,
		"max-len": 0,
		"no-else-return": 0,
		"no-shadow": 0,
		"no-param-reassign": 0,
		"spaced-comment": 0,
		"prefer-const": 0,
		"no-underscore-dangle": 0,
		"import/no-unresolved": 0,
		"import/extensions": 0
	}
}